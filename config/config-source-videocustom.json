                , {"name": "cfgvidmininterval"          , "default": "minutes",  "type": "alphanum", "permission": "SOURCES_CONFIGURATION_SIMPLE", "description": "Interval of this minimum time (seconds or minutes)"}
            ]
        }, {
            "name": "youtube"
            , "permission": "SOURCES_CONFIGURATION_SIMPLE"
            , "parameters": [
                {"name": "cfgvideoyoutubeupload"    , "default": "no",           "type": "yesno",    "permission": "SOURCES_CONFIGURATION_SIMPLE", "description": "Upload video to youtube"}
                , {"name": "cfgvideoyoutubecategory", "default": "Education",    "type": "alphanum", "permission": "SOURCES_CONFIGURATION_SIMPLE", "description": "Youtube Category"}
            ]
        }, {
            "name": "audio"
            , "permission": "SOURCES_CONFIGURATION_SIMPLE"
            , "parameters": [
                {"name": "cfgvideoaddaudio"     , "default": "no",   "type": "yesno", "permission": "SOURCES_CONFIGURATION_SIMPLE", "description": "Add audio to the video"}
                , {"name": "cfgvideoaudiofile"  , "default": "",     "type": "alphanum", "permission": "SOURCES_CONFIGURATION_SIMPLE", "description": "Audio file or playlist to be added"}
            ]
        }, {
            "name": "email"
            , "permission": "SOURCES_CONFIGURATION_SIMPLE"
            , "parameters": [
                {"name": "cfgvideoemailactivate"     , "default": "no",   "type": "yesno", "permission": "SOURCES_CONFIGURATION_SIMPLE", "description": "Send email once video creation is completed"}
            ]
        }, {
            "name": "flicker"
            , "permission": "SOURCES_CONFIGURATION_SIMPLE"
            , "parameters": [
                {"name": "cfgdeflickeractivate" , "default": "no", "type": "yesno", "permission": "SOURCES_CONFIGURATION_SIMPLE", "description": "Activate (or not) deflicker on the video"}
