                        , {"name": "insertXLocation"    , "default": "10",       "type": "int",      "description": "X Location"}
                        , {"name": "insertYLocation"    , "default": "100",      "type": "int",      "description": "Y Location"}
                    ]
                }
                , {"name": "cfgphidgetsensor4", "default": null, "type": "list",   "permission": "SOURCES_CONFIGURATION_SIMPLE", "description": "Configure sensor 4"
                    , "values": [
                        {"name": "phidgetSensor"        , "default": "",                     "type": "complex",  "description": "Phidget Sensor template"}
                        , {"name": "displayName"        , "default": "Temperature (degC)",   "type": "complex",  "description": "Display name"}
                        , {"name": "displayColor"       , "default": "FF0000",               "type": "alphanum", "description": "Display color"}
                        , {"name": "insertPicture"      , "default": "no",       "type": "yesno",    "description": "Insert in picture (yes or no)"}
                        , {"name": "insertSize"         , "default": "800x250",  "type": "complex",  "description": "Insert size"}
                        , {"name": "insertTransparency" , "default": "70",       "type": "int",      "description": "Transparency"}
                        , {"name": "insertXLocation"    , "default": "10",       "type": "int",      "description": "X Location"}
                        , {"name": "insertYLocation"    , "default": "100",      "type": "int",      "description": "Y Location"}
                    ]
                }
            ]
